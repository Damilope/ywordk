{
  "name": "ywordk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start-dev": "next dev -p $PORT",
    "start": "next start -p $PORT",
    "build": "next build",
    "dev": "env-cmd -f \".env.dev\" npm run start-dev",
    "build-dev": "env-cmd -f \".env.dev\" npm run build",
    "dev-prod": "env-cmd -f \".env.production\" npm run start-dev",
    "build-prod": "env-cmd -f \".env.production\" npm run build",
    "sync-public": "env-cmd -f \".env.production\" zx sh/sync-public.mjs",
    "start-prod": "npm run build-prod && env-cmd -f \".env.production\" npm run start",
    "lint": "next lint"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.3",
    "@types/mdx": "^2.0.13",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "next": "14.2.3",
    "next-mdx-remote": "^5.0.0",
    "node-fetch": "^3.3.2",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.9.0",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "zx": "^8.1.5"
  }
}
